deffindAnd(r,dict,str):
print("__debutand__",str)
positionOP=str.find('+')
ifpositionOP==-1:
returnstr
letter1=str[positionOP-1]
letter2=str[positionOP+1]
print("letter1",letter1)
print("bool",r.alpha[letter1]['val'])
result=r.alpha[letter1]["val"]andr.alpha[letter2]["val"]
print("result",result)
ifresult==True:
result="1"
else:
result="0"
sub=str.replace(letter1+"+"+letter2,result,1)

print("sub",sub)
newstr=solveExp(r,dict,sub)
print("newstr",newstr)
returnnewstr

deffindExclamation(r,dict,str):
print("__debutexclamation__",str)
positionOP=str.find('!')
ifpositionOP==-1:
returnstr
letter=str[positionOP+1]
print("letter1",letter)
print("bool",r.alpha[letter]['val'])
result=notr.alpha[letter]["val"]
print("result",result)
ifresult==True:
result="1"
else:
result="0"
sub=str.replace("!"+letter,result,1)

print("sub",sub)
newstr=solveExp(r,dict,sub)
print("newstr",newstr)
returnnewstr

