deffindParanthese(r,dict,str):
position1=str.find('(')
ifposition1==-1:
returnstr
tmp=1
lenght=len(str)
print("len",lenght)
i=position1+1
print("i",i)
print("str",str)
position2=0
while(i<lenght):
print("str[i]",str[i])
print("tmp",tmp)
ifstr[i]=='(':
tmp+=1
elifstr[i]==')'andtmp==1:
position2=i
break
elifstr[i]==')'andtmp!=1:
tmp-=1
i+=1
print("position2",position2)
sub=str[position1+1:position2]
print("sub",sub)
ret=solveExp(r,dict,sub)
newstr=str.replace("("+sub+")",ret,1)
print("printstr",newstr)
returnnewstr

